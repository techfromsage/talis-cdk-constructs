    "aws-cdk": "2.1018.0",
    "aws-sdk": "^2.1363.0",
    "axios": "^1.8.3",
    "constructs": "^10.0.0",
    "conventional-changelog-conventionalcommits": "^5.0.0",
    "esbuild": "^0.19.3",
    "eslint": "^8.57.1",
