    "husky": "^8.0.3",
    "jest": "^29.6.4",
    "prettier": "^3.5.3",
    "semantic-release": "^18.0.1",
    "talis-node": "github:talis/talis-node#v0.2.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "~5.2.2",
    "uuid": "^9.0.1"
